<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.spring.homeshare.MemberMapper">

	<!-- 로그인 Sql -->
	<select id="login" resultType="MemberVO">
		select * from ${house_member}
		where ${memId} = #{memId} and
		${password} = #{password}
	</select>
	
	<insert id="insert" parameterType="memberVO">
		insert into house_member values(0,#{memId},#{password},#{houseNo,jdbcType=VARCHAR},#{account},#{memName},#{email})

	</insert>
	
	
	<!-- 로그아웃 -->
	<select id="">
	</select>
	<!-- 아이디 찾기 찾기 sql -->
	<select id="findId" resultType="String">
		select ${memId} from ${house_member}
		where ${memName} = #{memName} and
		${email} = #{email}
	</select>
	

</mapper>










